{
    "name": "eserozvataf/scabbia2-router",
    "description": "Scabbia2 Router Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "eserozvataf",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "router",
        "routing"
    ],
    "homepage": "https://github.com/eserozvataf/scabbia2",
    "license": [
        "Apache-2.0",
        "BSD-3-Clause"
    ],
    "authors": [
        {
            "name": "Eser Ozvataf",
            "homepage": "http://eser.ozvataf.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/eserozvataf/scabbia2-router/blob/master/contributors.md"
        },
        {
            "name": "Nikita Popov",
            "email": "nikic@php.net"
        }
    ],
    "support": {
        "issues": "https://github.com/eserozvataf/scabbia2-router/issues",
        "source": "https://github.com/eserozvataf/scabbia2-router"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "eserozvataf/scabbia2-tasks": "*",
        "eserozvataf/scabbia2-testing": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Router\\": [ "src/Router/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Router\\": [ "tests/Router/" ]
        }
    }
}
